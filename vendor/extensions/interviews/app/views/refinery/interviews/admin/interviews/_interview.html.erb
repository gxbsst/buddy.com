<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(interview) -%>">
  <span class='title'>
    <%= interview.people_name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.interviews_interview_path(interview),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_interviews_admin_interview_path(interview),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.interviews_admin_interview_path(interview),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => interview.people_name),
        :method => :delete %>
  </span>
</li>